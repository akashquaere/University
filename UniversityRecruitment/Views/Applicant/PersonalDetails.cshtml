                        <div class="col-sm-3">
                            <div class="form-group">
                                <label class="col-sm-12 required">Upload AADHAR</label>
                                <div class="col-sm-12">
                                    <input type="file" class="form-control">

                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

